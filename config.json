{
  "dns": {
    "servers": [
      {"tag": "local", "type": "udp", "server": "223.5.5.5"},
      {"tag": "public", "type": "https", "server": "dns.alidns.com", "domain_resolver": "local"},
      {"tag": "google", "type": "https", "server": "8.8.8.8", "detour": "默认代理"}
    ],
    "rules": [
      {"clash_mode": "direct", "server": "local"},
      {"clash_mode": "global", "server": "fakeip"},
      {"query_type": "HTTPS", "action": "reject"},
      {"domain_suffix": ["services.googleapis.cn", "googleapis.cn", "xn--ngstr-lra8j.com"], "server": "google"},
      {"rule_set": ["cn_domain"], "server": "local"}
    ],
    "final": "google",
    "strategy": "prefer_ipv4",
    "independent_cache": true,
    "reverse_mapping": true
  },
  "outbounds": [
    {"tag": "默认代理", "type": "selector", "outbounds": []},
    {"tag": "漏网之鱼", "type": "selector", "outbounds": ["默认代理", "直连"]},
    {"tag": "自动选择", "type": "urltest", "outbounds": []},
    {"tag": "GLOBAL", "type": "selector", "outbounds": ["默认代理", "直连"]},
    {"tag": "直连", "type": "direct"}, 
    {"tag": "拦截", "type": "block"}
  ],
  "route": {
    "rules": [
      {"action": "sniff", "sniffer": ["http", "tls", "quic", "dns"]},
      {"inbound": "dns-in", "action": "hijack-dns"},
      {"ip_is_private": true, "outbound": "直连"},
      {"clash_mode": "direct", "outbound": "直连"},
      {"clash_mode": "global", "outbound": "GLOBAL"},
      {"rule_set": "ads", "outbound": "拦截"},
      {"rule_set": "medirect", "outbound": "直连"},
      {"rule_set": "meproxy", "outbound": "默认代理"},
      {"rule_set": "foreign_domain", "outbound": "默认代理"},
      {"rule_set": "cn_domain", "outbound": "直连"},
      {"rule_set": "geoip-google", "outbound": "默认代理"},
      {"rule_set": "geoip-telegram", "outbound": "默认代理"},
      {"rule_set": "geoip-cn", "outbound": "直连"}
    ],
    "rule_set": [
      {"tag": "ads", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/217heidai/adblockfilters/main/rules/adblocksingbox.srs", "download_detour": "默认代理"},
      {"tag": "medirect", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/evecus/files/refs/heads/main/singbox-rules/direct.srs", "download_detour": "默认代理"},
      {"tag": "meproxy", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/evecus/files/refs/heads/main/singbox-rules/proxy.srs", "download_detour": "默认代理"},
      {"tag": "foreign_domain", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/evecus/files/refs/heads/main/singbox-rules/foreign_domain.srs", "download_detour": "默认代理"},
      {"tag": "cn_domain", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs", "download_detour": "默认代理"},
      {"tag": "geoip-google", "type": "remote", "format": "binary", "url": "https://gh-proxy.com/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/google.srs", "download_detour": "默认代理"},
      {"tag": "geoip-telegram", "type": "remote", "format": "binary", "url": "https://gh-proxy.com/https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/telegram.srs", "download_detour": "默认代理"},
      {"tag": "geoip-cn", "type": "remote", "format": "binary", "url": "https://gh-proxy.com/https://github.com/qljsyph/ruleset-icon/raw/refs/heads/main/sing-box/geoip/China-ASN-combined-ip.srs", "download_detour": "默认代理"}
    ],
    "final": "漏网之鱼",
    "default_domain_resolver": {"server": "google"}
  },
  "inbounds": [
    {
      "tag": "dns-in",
      "type": "direct",
      "listen": "::",
      "listen_port": 1053
    },
    {
      "tag": "http-in",
      "type": "http",
      "listen": "::",
      "listen_port": 7890
    },
    {
      "tag": "tproxy-in",
      "type": "tproxy",
      "listen": "::",
      "listen_port": 7893
    }
  ],
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "/opt/singbox/cache.db",
      "store_fakeip": false
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/opt/singbox/ui",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "默认代理",
      "secret": "1234",
      "default_mode": "rule"
    }
  },
  "log": {
    "disabled": true
  }
}
