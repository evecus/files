{
  "dns": {
    "servers": [
      {
        "tag": "google",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "PROXY"
      },
      {
        "tag": "ali",
        "type": "https",
        "server": "223.5.5.5"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "action": "route",
        "server": "ali"
      },
      {
        "clash_mode": "global",
        "action": "route",
        "server": "fakeip"
      },
      {
        "query_type": "HTTPS",
        "action": "reject"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "action": "route",
        "server": "fakeip",
        "rewrite_ttl": 1
      },
      {
        "rule_set": [
          "cn_domain"
        ],
        "action": "route",
        "server": "ali"
      }
    ],
    "final": "google",
    "strategy": "prefer_ipv4",
    "independent_cache": true,
    "reverse_mapping": true
  },
  "inbounds": [
    {
      "tag": "dns-in",
      "type": "direct",
      "listen": "::",
      "listen_port": 1053
    },
    {
      "tag": "tproxy-in",
      "type": "tproxy",
      "listen": "::",
      "listen_port": 7893
    }
  ],
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "/opt/singbox/cache.db",
      "store_fakeip": false
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/opt/singbox/ui",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "默认代理",
      "secret": "1234",
      "default_mode": "rule"
    }
  },
  "route": {
    "rules": [
      {
        "action": "sniff",
        "sniffer": [
          "http",
          "tls",
          "quic",
          "dns"
        ]
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "直连"
      },
      {
        "clash_mode": "direct",
        "outbound": "直连"
      },
      {
        "clash_mode": "global",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": "ads",
        "outbound": "拦截"
      },
      {
        "rule_set": "medirect",
        "outbound": "直连"
      },
      {
        "rule_set": "meproxy",
        "outbound": "默认代理"
      },
      {
        "rule_set": "foreign",
        "outbound": "默认代理"
      },
      {
        "rule_set": "cn_domain",
        "outbound": "直连"
      },
      {
        "action": "resolve"
      },
      {
        "rule_set": "cn-ip",
        "outbound": "直连"
      },
      {
        "rule_set": "geoip-telegram",
        "outbound": "默认代理"
      },
      {
        "rule_set": "geoip-google",
        "outbound": "默认代理"
      }
    ],
    "rule_set": [
      {
        "tag": "ads",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/217heidai/adblockfilters/main/rules/adblocksingbox.srs",
        "download_detour": "默认代理"
      },
      {
        "tag": "medirect",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/evecus/files/refs/heads/main/singbox-rules/direct.srs",
        "download_detour": "默认代理"
      },
      {
        "tag": "meproxy",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/evecus/files/refs/heads/main/singbox-rules/proxy.srs",
        "download_detour": "默认代理"
      },
      {
        "tag": "foreign",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/evecus/files/refs/heads/main/singbox-rules/foreign_domain.srs",
        "download_detour": "默认代理"
      },
      {
        "tag": "cn_domain",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs",
        "download_detour": "默认代理"
      },
      {
        "tag": "geoip-google",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/google.srs",
        "download_detour": "默认代理"
      },
      {
        "tag": "geoip-telegram",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/telegram.srs",
        "download_detour": "默认代理"
      },
      {
        "tag": "cn-ip",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geoip/cn.srs",
        "download_detour": "默认代理"
      }
    ],
    "final": "漏网之鱼",
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "google"
    }
  },
  "log": {
    "disabled": true
  },
  "outbounds": [
    {
      "tag": "默认代理",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "自动选择",
      "type": "urltest",
      "outbounds": []
    },
    {
      "tag": "漏网之鱼",
      "type": "selector",
      "outbounds": [
        "直连",
        "默认代理"
      ]
    },
    {
      "tag": "直连",
      "type": "direct"
    },
    {
      "tag": "拦截",
      "type": "block"
    }
  ]
}



